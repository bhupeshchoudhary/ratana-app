{
  "expo": {
    "name": "Ratana - Retailer Order App",
    "slug": "ratana-retailer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ratana.jpg",
    "scheme": "ratanaretailer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show products available in your area."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ratana.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ratana.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Ratana to use your location to show available products in your area."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ratana.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4d06dd8d-5d6e-4333-bcc9-d5d7a21e0305"
      }
    },
    "owner": "bhupesh7750"
  }
}